@model VideoSharing.Areas.Member.ViewModels.MyVideosEdit

<h1>Edit Video @Model.title</h1>

@using (Html.BeginForm())
{
    @Html.ValidationSummary()

    <div class="form-group">
        @Html.LabelFor(f => f.title)
        @Html.TextBoxFor(f => f.title, new { @class = "form-control" })

    </div>

    <input type="submit" value="Update Video" class="btn btn-primary" />
    <a href="@Url.Action("index")">or cancel</a>
}
